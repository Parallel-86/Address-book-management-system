<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.parallel.module.register.Register">
   <children>
      <HBox layoutX="-1.0" layoutY="-1.0" opacity="0.65" prefHeight="128.0" prefWidth="1002.0" style="-fx-background-color: #3399CC; -fx-alignment: center;">
         <children>
            <Label prefHeight="61.0" prefWidth="480.0" text="用  户  信  息  注  册" textFill="#1307f7">
               <font>
                  <Font name="System Bold" size="48.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Label layoutX="277.0" layoutY="167.0" prefHeight="55.0" prefWidth="119.0" text="姓 名：" textFill="#d02e2e">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <TextField fx:id="name" layoutX="374.0" layoutY="172.0" prefHeight="25.0" prefWidth="305.0" promptText="你的名字">
         <font>
            <Font size="23.0" />
         </font>
      </TextField>
      <Label layoutX="277.0" layoutY="235.0" prefHeight="55.0" prefWidth="119.0" text="密 码：" textFill="#dd0808">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <TextField fx:id="pwd" layoutX="374.0" layoutY="240.0" prefHeight="25.0" prefWidth="305.0" promptText="你的密码">
         <font>
            <Font size="23.0" />
         </font>
      </TextField>
      <Label layoutX="277.0" layoutY="304.0" prefHeight="55.0" prefWidth="119.0" text="地 址：" textFill="#e82929">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <TextField fx:id="address" layoutX="375.0" layoutY="309.0" prefHeight="25.0" prefWidth="305.0" promptText="你的地址">
         <font>
            <Font size="23.0" />
         </font>
      </TextField>
      <Label layoutX="228.0" layoutY="375.0" prefHeight="51.0" prefWidth="160.0" text="联系方式：" textFill="#e12929">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <TextField fx:id="phone" layoutX="374.0" layoutY="378.0" prefHeight="25.0" prefWidth="305.0" promptText="你的联系方式">
         <font>
            <Font size="23.0" />
         </font>
      </TextField>
      <RadioButton fx:id="man" layoutX="397.0" layoutY="458.0" mnemonicParsing="false" text="男">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="sex" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="woman" layoutX="527.0" layoutY="458.0" mnemonicParsing="false" selected="true" text="女" toggleGroup="$sex">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </RadioButton>
      <Button layoutX="559.0" layoutY="549.0" mnemonicParsing="false" onAction="#login" prefHeight="51.0" prefWidth="142.0" style="-fx-background-color: blue;" text="返   回" textFill="#f8e6e6">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
      <Button layoutX="295.0" layoutY="549.0" mnemonicParsing="false" onAction="#register" prefHeight="51.0" prefWidth="142.0" style="-fx-background-color: blue;" text="注   册" textFill="WHITE">
